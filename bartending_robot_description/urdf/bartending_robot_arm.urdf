<?xml version="1.0" ?>
<robot name="bartending_robot_arm">
    
    <!-- platform to base -->
    <link name="base_platform">
        <visual>
            <geometry><box size="0.2 0.12 0.015" /></geometry>
        </visual>
        <!-- <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="5"/>
            <inertia
                ixx="1.0" ixy="0.0" ixz="0.0"
                iyy="1.0" iyz="0.0"
                izz="1.0"/>
        </inertial> -->
        <collision>
            <geometry><box size="0.2 0.12 0.015" /></geometry>
        </collision>
    </link>

    <link name="base_link">
        <visual>
            <geometry><box size="0.06 0.04 0.043" /></geometry>
        </visual>
        <!-- <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="1"/>
            <inertia
                ixx="1.0" ixy="0.0" ixz="0.0"
                iyy="1.0" iyz="0.0"
                izz="1.0"/>
        </inertial> -->
        <collision>
            <geometry><box size="0.06 0.04 0.043" /></geometry>
        </collision>
    </link>

    <joint name="base_mount" type="fixed">
        <origin xyz="0.043 0 0.029"/>
        <parent link="base_platform"/>
        <child link="base_link"/>
    </joint>

    <!-- base to shoulder -->
    <link name="base_link_offset">
        <visual>
            <origin xyz="0 0 0" />
            <geometry><cylinder radius="0.015" length="0.025" /></geometry>
        </visual>
        <!-- <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.1"/>
            <inertia
                ixx="1.0" ixy="0.0" ixz="0.0"
                iyy="1.0" iyz="0.0"
                izz="1.0"/>
        </inertial> -->
        <collision>
            <origin xyz="0 0 0" />
            <geometry><cylinder radius="0.015" length="0.025" /></geometry>
        </collision>
    </link>

    <joint name="base_z_rot" type="revolute">
        <origin xyz="-0.011 0 0.034"/>
        <parent link="base_link"/>
        <child link="base_link_offset"/>
        <axis xyz="0 0 1"/>
        <limit effort="0" lower="-3.14" upper="3.14" velocity="0.2"/>
    </joint>

    <!-- shoulder to elbow -->
    <link name="upper_link">
        <visual>
            <origin xyz="0 0 0.034" />
            <geometry><box size="0.03 0.05 0.068" /></geometry>
        </visual>
        <!-- <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.15"/>
            <inertia
                ixx="1.0" ixy="0.0" ixz="0.0"
                iyy="1.0" iyz="0.0"
                izz="1.0"/>
        </inertial> -->
        <collision>
            <origin xyz="0 0 0.034" />
            <geometry><box size="0.03 0.05 0.068" /></geometry>
        </collision>
    </link>

    <joint name="base_y_rot" type="revolute">
        <origin xyz="-0.0075 0 0.0125"/>
        <parent link="base_link_offset"/>
        <child link="upper_link"/>
        <axis xyz="0 1 0"/>
        <limit effort="0" lower="-1.007" upper="1.447" velocity="0.2"/>
    </joint>

    <!-- elbow to wrist -->
    <link name="lower_link">
        <visual>
            <origin xyz="0 0 0.034" />
            <geometry><box size="0.03 0.05 0.068" /></geometry>
        </visual>
        <!-- <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.15"/>
            <inertia
                ixx="1.0" ixy="0.0" ixz="0.0"
                iyy="1.0" iyz="0.0"
                izz="1.0"/>
        </inertial> -->
        <collision>
            <origin xyz="0 0 0.034" />
            <geometry><box size="0.03 0.05 0.068" /></geometry>
        </collision>
    </link>

    <joint name="elbow_y_rot" type="revolute">
        <origin xyz="0 0 0.068"/>
        <parent link="upper_link"/>
        <child link="lower_link"/>
        <axis xyz="0 1 0"/>
        <limit effort="0" lower="-1.687" upper="1.6619" velocity="0.2"/>
    </joint>

    <!-- wrist to tooltip -->
    <link name="end_effector_link">
        <visual>
            <origin xyz="0 0 0.034" />
            <geometry><box size="0.03 0.05 0.068" /></geometry>
        </visual>
        <!-- <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.15"/>
            <inertia
                ixx="1.0" ixy="0.0" ixz="0.0"
                iyy="1.0" iyz="0.0"
                izz="1.0"/>
        </inertial> -->
        <collision>
            <origin xyz="0 0 0.034" />
            <geometry><box size="0.03 0.05 0.068" /></geometry>
        </collision>
    </link>

    <joint name="wrist_y_rot" type="revolute">
        <origin xyz="0 0 0.068"/>
        <parent link="lower_link"/>
        <child link="end_effector_link"/>
        <axis xyz="0 1 0"/>
        <limit effort="0" lower="-1.713" upper="1.6413" velocity="0.2"/>
    </joint>

    <!-- tooltip -->
    <link name="tooltip_mount">
        <visual>
            <origin xyz="0 0 0" />
            <geometry><box size="0.05 0.05 0.029" /></geometry>
        </visual>
        <!-- <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.15"/>
            <inertia
                ixx="1.0" ixy="0.0" ixz="0.0"
                iyy="1.0" iyz="0.0"
                izz="1.0"/>
        </inertial> -->
        <collision>
            <origin xyz="0 0 0" />
            <geometry><box size="0.05 0.05 0.029" /></geometry>
        </collision>
    </link>

    <joint name="gripper_mount" type="fixed">
        <origin xyz="-0.012 0 0.0825"/>
        <parent link="end_effector_link"/>
        <child link="tooltip_mount"/>
    </joint>

    <!-- gripper component -->
    <link name="gripper_link">
        <visual>
            <origin xyz="0 0 0.0645" />
            <geometry><box size="0.03 0.1 0.105" /></geometry>
        </visual>
        <!-- <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.15"/>
            <inertia
                ixx="1.0" ixy="0.0" ixz="0.0"
                iyy="1.0" iyz="0.0"
                izz="1.0"/>
        </inertial> -->
        <collision>
            <origin xyz="0 0 0.0645" />
            <geometry><box size="0.05 0.11 0.1" /></geometry>
        </collision>
    </link>

    <joint name="gripper_z_rot" type="revolute">
        <origin xyz="-0.015 0 0"/>
        <parent link="tooltip_mount"/>
        <child link="gripper_link"/>
        <axis xyz="0 0 1"/>
        <limit effort="0" lower="-3.14" upper="3.14" velocity="0.2"/>
    </joint>


</robot>